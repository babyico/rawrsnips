{
  "Changelog init (full) ~ Rawr": {
    "description": "Full Changelog sample ~ Rawr",
    "prefix": ["cl-full","changelog-init-full", "clinitfull"],
    "body": [
      "# Changelog üìù",
      "${1:",
      "## [Unreleased]",
      "",
      "### Changed",
      "",
      "- ${2:TODO}}",
      "",
      "## [${3:Version}] - $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE",
      "${4:",
      "### Added",
      "",
      "- ${5:Add item}}",
      "${6:",
      "### Changed",
      "",
      "- ${7:Change item}}",
      "${8:",
      "### Fixed",
      "",
      "- ${9:Fixed item}}",
      "${10:",
      "### Removed",
      "",
      "- ${11:Remove item}}",
      "${12:",
      "### Security",
      "",
      "- ${13:Security item}}"
    ]
  },
  "Changelog init (simple) ~ Rawr": {
    "description": "Simple Changelog sample ~ Rawr",
