  "Change log init New Version (No fields) ~ Rawr": {
    "description": "Log a new empty version ~ Rawr",
    "prefix": ["cl-version","clversion"],
    "body": [
      "",
    "## [${1:Version}] - $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE",
    "${2:",
    ""
   ]
  },
  "Change log init New Version ~ Rawr": {
    "description": "Log a new version ~ Rawr",
    "prefix": ["cl-version-full","changelog-new-version", "clnew"],
    "body": [
