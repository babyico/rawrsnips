      "## [${1:Version}] - $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE",
      "${2:",
      "### Added",
      "",
      "- ${3:Add item}}",
      "${4:",
      "### Changed",
      "",
      "- ${5:Change item}}",
      "${6:",
      "### Fixed",
      "",
      "- ${7:Fixed item}}",
      "${8:",
      "### Removed",
      "",
      "- ${9:Remove item}}",
      "${10:",
      "### Security",
      "",
      "- ${11:Security item}}"
    ]
  },
