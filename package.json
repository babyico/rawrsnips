  "version": "1.0.2",
  "displayName": "RawrSnips",
  "publisher": "Iconical",
  "icon": "images/rawrsnips.png",
  "license": "LICENSE.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/babyico/rawrsnips"
  },
  "engines": {
    "vscode": "0.10.x"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
